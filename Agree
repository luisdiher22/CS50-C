#include <cs50.h>
#include <stdio.h>

int main (void)
{
        //Asking the user to agree
        char c = get_char("Do you agree?");

        //Checks if the user agreed

        //Use || in order to prevent the error in which the user types with  Capslocked
        if(c == 'y' || c == 'Y')
        {
            printf("Agreed.\n");
        }
        else if (c == 'n' || c == 'N')
        {
            printf("Not agreed.\n");
        }
}
